extends layout

block content
  #header-container
    h2= project.name
    p= project.dateHtml
    p(style='color: #aaa;') Click a photo to download.
    //- a(onclick='zipImages()').btn.btn-default Download All
  br
  #gallery-container
    .grid  
      each photo in photos
        if photo.deleted !== true
          .grid-item
            a(href='/uploads/' + photo.name, download= photo.originalName)
              img(src= '/uploads/' + photo.name).image
      
